{
    "name": "tabbr",
    "description": "save your open tabs in your workspace and restore them whenever you want",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs","storage","tabGroups"
      ]
  }